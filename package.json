  "license": "MIT",
  "scripts": {
    "cdk-deploy:shared": "cross-env APP_TYPE=shared cdk deploy \"Shared\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/shared.ts\" --require-approval never --outputs-file cdk.shared.out.json --profile=bob",
    "cdk-deploy:backend": "cross-env APP_TYPE=backend cdk deploy \"Backend\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/backend.ts\" --require-approval never --outputs-file cdk.backend.out.json --profile=bob",
    "cdk-deploy:frontend": "cross-env APP_TYPE=frontend cdk deploy \"Frontend\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/frontend.ts\" --require-approval never --outputs-file cdk.frontend.out.json --profile=bob",
    "cdk-destroy:shared": "cross-env APP_TYPE=shared cdk destroy \"Shared\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/shared.ts\" --force --profile=bob",
    "cdk-destroy:backend": "cross-env APP_TYPE=backend cdk destroy \"Backend\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/backend.ts\" --force --profile=bob",
    "cdk-destroy:frontend": "cross-env APP_TYPE=frontend cdk destroy \"Frontend\" --app \"node -r ts-node/register --max-old-space-size=6144 infrastructure/frontend.ts\" --force --profile=bob",
    "cdk-deploy:all": "npm run cdk-deploy:shared && npm run cdk-deploy:backend && npm run cdk-deploy:frontend",
    "api-local": "(cd infrastructure/local && rm -rf dist && esbuild ./src/main.ts --sourcemap --platform=node --bundle --outfile=dist/main.js) && node ./infrastructure/local/dist/main.js"
  },
  "private": true,
